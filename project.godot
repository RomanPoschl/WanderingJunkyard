; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="WanderingJunkyard"
run/main_scene="res://Game/Game.tscn"
config/features=PackedStringArray("4.0", "C#")
config/icon="res://icon.png"

[autoload]

SceneChanger="*res://SceneChanger.tscn"
Universe="*res://Game/Universe/Universe.cs"
Events="*res://Autoload/Events.cs"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/handheld/orientation=1
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/size/width=480
window/size/height=720

[dotnet]

project/assembly_name="WanderingJunkyard"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[gui]

common/drop_mouse_on_gui_input_disabled=true

